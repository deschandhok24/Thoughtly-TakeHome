{
  "name": "ticket-system",
  "version": "1.0.0",
  "type": "module",
  "description": "Concert ticket booking system with Express, Postgres, and Prisma",
  "main": "src/index.ts",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
